{
    "name": "nmred/kafka-php",
    "description": "Kafka client for php",
    "type": "library",
    "keywords": [
        "Client",
        "Kafka"
    ],
    "homepage": "http://www.swanlinux.net",
    "license": "BSD-3-Clause",
    "config": {
        "platform": {
            "php": "7.1.12"
        },
        "preferred-install": "dist",
        "sort-packages": true
  },
    "require": {
        "php": "^7.1",
        "amphp/amp": "^1.2",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Kafka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "KafkaTest\\": "tests/",
            "KafkaExample\\": "example/"
        }
    },
    "require-dev": {
        "kmelia/monolog-stdout-handler": "^1.2",
        "monolog/monolog": "^1.23",
        "phpunit/phpunit": "^5.7",
        "phpunit/phpcov": "^3.1",
        "satooshi/php-coveralls": "2.0.x-dev@dev"
    }
}
